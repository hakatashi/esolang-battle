    "@babel/preset-react": "^7.0.0",
    "array-shuffle": "^1.0.1",
    "babel-loader": "^8.0.4",
    "bcrypt-nodejs": "^0.0.3",
    "bignumber.js": "^8.0.1",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.3",
    "chalk": "^2.4.1",
    "classnames": "^2.2.6",
    "common-tags": "^1.8.0",
    "compression": "^1.7.3",
    "concat-stream": "^1.6.2",
    "connect-mongo": "^2.0.1",
