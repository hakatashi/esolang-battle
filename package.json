    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "reactstrap": "^6.5.0",
    "shell-escape": "^0.2.0",
    "socket.io": "^2.1.1",
    "three": "^0.98.0",
    "three-orbitcontrols": "^2.96.3",
    "three-trackballcontrols": "0.0.7",
    "tmp": "^0.0.33",
    "utf-8-validate": "^5.0.1",
    "validator": "^10.9.0",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3"
  },
  "devDependencies": {
    "@hakatashi/eslint-config": "^1.8.3",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.6",
    "prettier-eslint-cli": "^4.7.1",
    "sinon": "^7.1.1",
    "sinon-mongoose": "^2.2.1",
    "supertest": "^3.3.0"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "comma-dangle": 0,
      "consistent-return": 0,
      "linebreak-style": 0,
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "no-shadow": 0,
      "no-console": 0,
      "no-plusplus": 0
    }
  }
}
