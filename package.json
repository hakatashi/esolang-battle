    "garlicjs": "^1.4.2",
    "hexdump-stream": "pose/hexdump-stream",
    "jstransformer-scss": "^1.0.0",
    "lodash": "^4.17.11",
    "lusca": "^1.6.1",
    "markdown-it": "^8.4.2",
    "material-colors": "^1.2.6",
    "mathjs": "^5.2.3",
    "moment": "^2.22.2",
    "mongoose": "^5.3.12",
    "morgan": "^1.9.1",
    "multer": "^2.0.0-alpha.6",
    "node-sass-middleware": "^0.11.0",
